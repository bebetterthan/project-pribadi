{
  "name": "ai-pentest-agent",
  "version": "1.0.0",
  "description": "AI-powered penetration testing platform",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel start:backend start:frontend",
    "start:backend": "cd backend && ./start.sh",
    "start:frontend": "cd frontend && npm run dev",
    "install:backend": "cd backend && python -m venv venv && source venv/bin/activate && pip install -r requirements.txt",
    "install:frontend": "cd frontend && npm install",
    "install:all": "npm-run-all install:backend install:frontend"
  },
  "keywords": [
    "pentesting",
    "security",
    "ai",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "react-hot-toast": "^2.6.0"
  }
}
